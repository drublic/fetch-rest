{
  "name": "fetch-rest-connect",
  "version": "2.2.4",
  "main": "dist/index.js",
  "repository": "https://github.com/drublic/fetch-rest-connect.git",
  "author": "Hans Christian Reinl <info@drublic.de>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint **/*.js",
    "build": "rm -rf dist/ && babel . --ignore node_modules,coverage -d dist",
    "test": "jest",
    "prepublishOnly": "yarn build"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./jestSetup.js"
    ],
    "testURL": "http://localhost",
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.8.0",
    "jest-fetch-mock": "^2.1.2",
    "regenerator-runtime": "^0.13.3"
  },
  "dependencies": {
    "whatwg-fetch": "^3.0.0"
  }
}
